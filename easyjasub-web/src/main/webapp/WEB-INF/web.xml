<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">

	<display-name>easyjasub</display-name>

	<servlet>
		<servlet-name>EasyJaSubServlet</servlet-name>
		<servlet-class>com.github.riccardove.easyjasub.EasyJaSubServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EasyJaSubServlet</servlet-name>
		<url-pattern>/easyjasub/*</url-pattern>
	</servlet-mapping>

</web-app>